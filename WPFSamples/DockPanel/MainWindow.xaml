<Window x:Class="DockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DockPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button Content="T1" DockPanel.Dock="Bottom"/>
        <Button Content="T2" DockPanel.Dock="Left"/>
        <Button Content="T4" DockPanel.Dock="Right"/>
        <Button Content="T5" DockPanel.Dock="Top"/>
        <Button Content="T3"/>
    </DockPanel>
</Window>
